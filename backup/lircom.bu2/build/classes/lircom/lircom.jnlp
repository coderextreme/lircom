<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="1.0" 
      codebase="http://schizophrenics.net/yottzumm/lircom"
      href="lircom.jnlp">
  <information>
    <title>LirCom Chat</title>
    <vendor>Schizophrenics.NET</vendor>
    <homepage href="http://schizophrenics.net/yottzumm/lircom"/>
    <description>Language Independent Relay Communications</description>
    <description kind="short">Peer to Peer Chat</description>
    <offline-allowed/>
  </information>
  <security>
        <all-permissions/>
  </security>
    <resources>
      <j2se href="http://java.sun.com/products/autodl/j2se" version="1.5+"/>
        <jar href="lircom.jar" main="true"/>
        <jar href="solitaire.jar"/>
    </resources>
  <application-desc main-class="lircom.MainWindow">
	<argument>lircom.Chat</argument>
  </application-desc>
</jnlp>
